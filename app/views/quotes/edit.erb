<div id="custom_title">Quote Editor</div>
<div id="custom_form"><%= form_for @quote do |q| %>
    <%= q.label :author %>
    <%= q.text_field :author, value: @quote.author.name %><br><br>
    <%= q.label :content %>
    <%= q.text_area :content %><br><br>
    <%= q.submit class: "custom_button"%>
<% end %></div>